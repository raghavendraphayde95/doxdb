{
	"$schema":"http://json-schema.org/draft-04/schema#",
	"id":"http://sample.trajano.net/horse.json#",
	"type":"object",
	"properties":{
		"name":{
			"type":"string"
		},
		"passport":{
			"type":"string"
		},
		"fei":{
			"type":"string"
		},
		"usef":{
			"type":"string"
		},
		"ec":{
			"type":"string"
		},
		"dateOfBirth":{
			"type":"string",
			"format":"date-time",
			"minLength":10,
			"maxLength":10
		},
		"gender":{
			"enum":[
				"mare",
				"gelding"
			]
		},
		"color":{
			"type":"string"
		},
		"breed":{
			"type":"string"
		},
		"sire":{
			"type":"string"
		},
		"sireDam":{
			"type":"string"
		}
	},
	"required":[
		"name"
	]
}