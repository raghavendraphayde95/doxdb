		<session>
			<ejb-name>XmlConfigurationProvider</ejb-name>
			<business-local>net.trajano.doxdb.ext.ConfigurationProvider</business-local>
			<ejb-class>net.trajano.doxdb.ext.XmlConfigurationProvider</ejb-class>
